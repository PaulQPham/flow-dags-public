{
  "type": "index_parallel",
  "spec": {
    "ioConfig": {
      "type": "index_parallel",
      "inputSource": {
        "type": "local",
        "baseDir": "/var/shared-data/har-2020",
        "filter": "*.json.gz"
      },
      "inputFormat": {
        "type": "json"
      },
      "appendToExisting": true
    },
    "tuningConfig": {
      "type": "index_parallel",
      "partitionsSpec": {
        "type": "dynamic"
      }
    },
    "dataSchema": {
      "dataSource": "private@detx-har-agent-summary",
      "granularitySpec": {
        "type": "uniform",
        "segmentGranularity": "DAY",
        "queryGranularity": "MINUTE",
        "intervals": ["2010-01-01/2021-01-01"],
        "rollup": true
      },
      "timestampSpec": {
        "column": "StatusChangeTimestamp",
        "format": "iso"
      },
      "metricsSpec": [
        {
          "type": "count",
          "name": "TransactionCount"
        },
        { "type" : "doubleSum", "name" : "SumClosePrice", "fieldName" : "ClosePrice" },
        { "type" : "doubleSum", "name" : "SumListPrice", "fieldName" : "ListPrice" }
      ],
      "dimensionsSpec": {
        "dimensions": [
          "AgentName",
          "AgentMlsId",
          "OfficeName",
          "OfficeMlsId",
          "StandardStatus",
          "PropertyType",
          "ActivityType"
        ]
      },
      "transformSpec": {
        "filter": {
          "type": "selector",
          "dimension": "BuyerOfficeName",
          "value": "Douglas Elliman Real Estate"
        },
        "transforms": [
          {
            "type": "expression",
            "name": "AgentName",
            "expression": "\"BuyerAgentFirstName\""
          },
          {
            "type": "expression",
            "name": "OfficeName",
            "expression": "\"BuyerOfficeName\""
          },
          {
            "type": "expression",
            "name": "OfficeMlsId",
            "expression": "'DETX01'"
          },
          {
            "type": "expression",
            "name": "AgentMlsId",
            "expression": "\"BuyerAgentMlsId\""
          },
          {
            "type": "expression",
            "name": "StatusChangeTimestamp",
            "expression": "timestamp_shift(\"StatusChangeTimestamp\",'PT1M',2)"
          },
          {
            "type": "expression",
            "name": "ActivityType",
            "expression": "'Buyer'"
          }
        ]
      }
    }
  }
}