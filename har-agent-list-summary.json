{
  "type": "index_parallel",
  "spec": {
    "ioConfig": {
      "type": "index_parallel",
      "inputSource": {
        "type": "local",
        "baseDir": "/var/shared-data/har-2020",
        "filter": "*.json.gz"
      },
      "inputFormat": {
          "type": "json"
      },
      "appendToExisting": false
    },
    "tuningConfig": {
      "type": "index_parallel",
      "partitionsSpec": {
        "type": "dynamic"
      }
    },
    "dataSchema": {
      "dataSource": "private@detx-har-agent-summary",
      "granularitySpec": {
        "type": "uniform",
        "segmentGranularity": "DAY",
        "queryGranularity": "DAY",
        "rollup": true
      },
      "timestampSpec": {
        "column": "StatusChangeTimestamp",
        "format": "iso"
      },
      "metricsSpec": [
        {
          "type": "count",
          "name": "TotalSales"
        },
        { "type" : "doubleSum", "name" : "SumClosePrice", "fieldName" : "ClosePrice" },
        { "type" : "doubleSum", "name" : "SumListPrice", "fieldName" : "ListPrice" },
        { "type" : "doubleMean", "name" : "MeanClosePSF", "fieldName" : "HAR_PriceSqFtSold"},
        { "type" : "doubleMean", "name" : "MeanDOM", "fieldName" : "DaysOnMarket" }
      ],
      "dimensionsSpec": {
        "dimensions": [
          "AgentName",
          "AgentMlsId",
          "OfficeName",
          "OfficeMlsId"
        ]
      },
      "transformSpec": {
        "filter": {
          "type":"and",
          "fields": [
            { "type": "selector", "dimension": "ListOfficeMlsId", "value": "DETX01" },
            { "type": "selector", "dimension": "PropertyType", "value": "Residential" }
          ]
        },
        "transforms": [
          {
            "type": "expression",
            "name": "AgentName",
            "expression": "\"ListAgentFullName\""
          },
          {
            "type": "expression",
            "name": "OfficeName",
            "expression": "\"ListOfficeName\""
          },
          {
            "type": "expression",
            "name": "OfficeMlsId",
            "expression": "\"ListOfficeMlsId\""
          },
          {
            "type": "expression",
            "name": "AgentMlsId",
            "expression": "\"ListAgentMlsId\""
          }
        ]
      }
    }
  }
}